# Development Environment Variables (Optional)
# Copy this to .env for local development overrides
# Most settings have defaults in application-dev.yaml

# Override database if needed (optional - defaults to localhost:5423)
# DATABASE_URL=jdbc:postgresql://localhost:5432/ghor_bari_local
# DATABASE_USERNAME=postgres
# DATABASE_PASSWORD=postgres

# Override JWT secret for local testing (optional)
# JWT_SECRET=local_dev_secret_key_for_testing_only

# Override server port if needed (optional - defaults to 8081)
# SERVER_PORT=9090

# Spring Profile (defaults to dev if not specified)
# SPRING_PROFILES_ACTIVE=dev
